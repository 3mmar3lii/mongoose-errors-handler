  "repository": {
    "type": "git",
    "url": "https://github.com/3mmar3lii/mongoose-errors-handler.git"
  },
  "bugs": {
    "url": "https://github.com/3mmar3lii/mongoose-errors-handler/issues"
  },
  "homepage": "https://github.com/3mmar3lii/mongoose-errors-handler#readme",
  "peerDependencies": {
